<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.triple.model.user">

	<select id="login" resultType="com.triple.model.user.User">
		SELECT
			F_ID,
			F_NAME,
			F_EMAIL
		FROM T_USER
		WHERE F_ID = #{f_id}
			AND F_PASS = #{f_pass}
			AND F_STATUS = 'Y'
	</select>

</mapper>